{
  "expo": {
    "name": "masjidlink",
    "slug": "masjidlink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/masjidlink.png",
    "scheme": "masjidlink",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "jsEngine": "hermes",
      "bundleIdentifier": "com.mayowadan.masjidlink"
    },
    "android": {
      "versionCode": 1,
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/masjidlink.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/masjidlink.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mayowadan.masjidlink"
    },
    "jsEngine": "hermes",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/masjidlink.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take profile pictures."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d9d620a9-2ea4-4943-bb7b-e23f30a23ad8"
      }
    },
    "owner": "mayowadan"
  }
}
